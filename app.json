{
  "expo": {
    "name": "medilux_frontend",
    "slug": "mediluxfrontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/main_logo1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/main_logo1.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7F6EE"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/main_logo1.png",
        "backgroundColor": "#F7F6EE"
      },
      "package": "com.anonymous.medilux_frontend"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "39b89f33-994f-4176-b62c-96155bb35c02"
      }
    },
    "owner": "jmkim463"
  }
}
